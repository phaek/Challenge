(λx.x³)

1³ = (S(K((S((S(KS))K))(K((SK)K)))))((S(K((S((S(KS))K))(K((SK)K)))))((S(K((S((S(KS))K))(K((SK)K)))))((SK)K)))
2³ = (S(K((S((S(KS))K))((S((S(KS))K))(K((SK)K))))))((S(K((S((S(KS))K))((S((S(KS))K))(K((SK)K))))))((S(K((S((S(KS))K))((S((S(KS))K))(K((SK)K))))))((SK)K)))
3³ = (S(K((S((S(KS))K))((S((S(KS))K))((S((S(KS))K))(K((SK)K)))))))((S(K((S((S(KS))K))((S((S(KS))K))((S((S(KS))K))(K((SK)K)))))))((S(K((S((S(KS))K))((S((S(KS))K))((S((S(KS))K))(K((SK)K)))))))((SK)K)))

3¹ β -> (S(K((x)        (0))))   ((S(K((x)        (0))))   ((S(K((x)        (0))))   I))
3² β -> (S(K((x)((x)    (0)))))  ((S(K((x)((x)    (0)))))  ((S(K((x)((x)    (0)))))  I))
